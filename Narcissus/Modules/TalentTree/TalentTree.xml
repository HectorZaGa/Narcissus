					<Anchors>
						<Anchor point="TOP" relativeKey="$parent" relativePoint="TOP" y="0"/>
					</Anchors>
				</Texture>
            </Layer>
            <Layer level="ARTWORK">
				<Texture parentKey="Divider" file="Interface\AddOns\Narcissus\Art\Modules\Showcase\ShadowDivider">
