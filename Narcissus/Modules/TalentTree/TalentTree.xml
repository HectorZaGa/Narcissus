                    <Anchors>
                        <Anchor point="TOPLEFT" relativeKey="$parent.SpecIconBorder" relativePoint="TOPLEFT"/>
                        <Anchor point="BOTTOMRIGHT" relativeKey="$parent.SpecIconBorder" relativePoint="BOTTOMRIGHT"/>
                    </Anchors>
