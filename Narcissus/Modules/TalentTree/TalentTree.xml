            </Layer>
        </Layers>
        <Scripts>
            <OnEnter method="OnEnter"/>
            <OnLeave method="OnLeave"/>
            <OnClick method="OnClick"/>
        </Scripts>
    </Button>

    <Frame name="NarciMiniTalentTree" mixin="NarciMiniTalentTreeMixin" frameStrata="FULLSCREEN" enableMouse="true" hidden="true">
