                    <Anchors>
                        <Anchor point="TOPLEFT" relativeKey="$parent.SpecIcon" relativePoint="TOPLEFT"/>
                        <Anchor point="BOTTOMRIGHT" relativeKey="$parent.SpecIcon" relativePoint="BOTTOMRIGHT"/>
