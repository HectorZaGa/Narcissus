                    <Anchors>
                        <Anchor point="CENTER" relativeKey="$parent" relativePoint="CENTER"/>
                    </Anchors>
                </FontString>
                <FontString parentKey="SpecName" inherits="NarciFontThin9" justifyH="CENTER" justifyV="MIDDLE" hidden="true">
